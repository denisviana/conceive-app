<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.gau">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launch"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Splash_Activity" android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login_Activity" android:theme="@style/NoactionbarLogin">

        </activity>

        <activity android:name=".Cadastrar_Activity" android:theme="@style/NoactionbarLogin">

        </activity>

        <activity android:name=".Dashboard_Activity">

        </activity>
        <activity
            android:name=".Projetos_Activity" android:theme="@style/ProjetosTheme"
            android:label="@string/title_activity_projetos_"
            android:parentActivityName=".Dashboard_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.gau.Dashboard_Activity" />
        </activity>

        <activity
            android:name=".Criar_Projeto_Activity" android:theme="@style/ProjetosTheme"
            android:label="Novo Projeto"
            android:parentActivityName=".Projetos_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.gau.Projetos_Activity" />
        </activity>

        <activity
            android:name=".Visualizacao_Projeto_Activity"
            android:parentActivityName=".Projetos_Activity"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Projetos_Activity"/>
        </activity>

        <activity
            android:name=".Meu_Portfolio_Activity"
            android:label="Meu Portfólio"
            android:parentActivityName=".Dashboard_Activity"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Dashboard_Activity"/>
        </activity>

        <activity
            android:name=".Acervo_Referecencias_Activity"
            android:label="Acervo de Referências"
            android:parentActivityName=".Dashboard_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Dashboard_Activity"/>
        </activity>

    </application>

</manifest>