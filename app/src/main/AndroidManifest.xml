<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.gau">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Dashboard_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Projetos_Activity" android:theme="@style/ProjetosTheme"
            android:label="@string/title_activity_projetos_"
            android:parentActivityName=".Dashboard_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.gau.Dashboard_Activity" />
        </activity>

        <activity
            android:name=".Criar_Projeto_Activity" android:theme="@style/ProjetosTheme"
            android:label="Novo Projeto"
            android:parentActivityName=".Projetos_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.gau.Projetos_Activity" />
        </activity>

    </application>

</manifest>